{
  "name": "cnn-metrics",
  "version": "0.5.0",
  "description": "CNN Metrics",
  "main": "lib/metrics.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
