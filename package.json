  "engines": {
    "node": ">=4.3.1"
  },
  "dependencies": {
    "debug": "2.6.9",
    "dogapi": "2.8.0",
    "lodash": "4.17.10",
    "metrics": "0.1.15",
    "nconf": "0.10.0"
  },
  "devDependencies": {
    "changelog-maker": "2.2.7",
    "esdoc": "0.5.2",
    "eslint": "3.18.0",
    "jq-cli-wrapper": "0.4.0",
    "jsonlint": "1.6.3",
    "npm-check-updates": "2.14.2"
  },
  "keywords": [
